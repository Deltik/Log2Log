<?xml version="1.0" ?>
<formats>
	<format id="adium">
		<name>Adium</name>
		<client>Adium</client>
		<class>Adium</class>
		<icon>:/images/services/Adium.png</icon>
		<support>
			<from>true</from>
			<to>true</to>
			<specificity>0</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>yes</timezone>
			<status>true</status>
			<system></system>
			<aliases>true</aliases>
			<group>true</group>
			<frominfo>Did you shoot Adiumy, the Adium duck?</frominfo>
			<toinfo></toinfo>
		</support>
	</format>
	<format id="deltikhuman">
		<name>Deltik Standard Non-XML Chat Log Format</name>
		<client>Deltik</client>
		<class>Deltik</class>
		<icon>:/images/services/Deltik.png</icon>
		<support>
			<from>true</from>
			<to>true</to>
                        <specificity>-2</specificity>
			<accuracy>true</accuracy>
			<time>yes</time>
			<timezone>yes</timezone>
			<status>true</status>
			<system>true</system>
			<aliases>true</aliases>
			<group>true</group>
			<frominfo></frominfo>
			<toinfo>This is a specialized format invented by Deltik to create readable yet detailed chat logs.</toinfo>
		</support>
	</format>
	<format id="gchat">
		<name>Gmail Chats (download)</name>
		<client>Gmail</client>
		<class>Gmail</class>
		<icon>:/images/services/GChat.png</icon>
		<support>
			<from>true</from>
			<to>false</to>
			<specificity>2</specificity>
			<accuracy>false</accuracy>
			<time>group</time>
			<timezone>group</timezone>
			<status>false</status>
			<system>false</system>
			<aliases>true</aliases>
			<group>true</group>
			<frominfo>Sign into Gmail</frominfo>
			<toinfo></toinfo>
		</support>
	</format>
	<format id="gv">
		<name>Google Voice (download)</name>
		<client>Google Voice</client>
		<class>GV</class>
		<icon>:/images/services/GVoice.png</icon>
		<support>
			<from>true</from>
			<to>false</to>
			<specificity>2</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>no</timezone>
			<status>false</status>
			<system>false</system>
			<aliases>true</aliases>
			<group>false</group>
			<frominfo>Sign into Google Voice</frominfo>
			<toinfo></toinfo>
		</support>
	</format>
	<format id="imo">
		<name>imo.im (download)</name>
		<client>imo instant messenger</client>
		<class>IMO</class>
		<icon>:/images/services/imo.png</icon>
		<support>
			<from>true</from>
			<to>false</to>
			<specificity>0</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>no</timezone>
			<status>false</status>
			<system>false</system>
			<aliases>true</aliases>
			<group>true</group>
			<frominfo>Sign in using your imo username. (Load from linked accounts not supported yet by Log2Log.)</frominfo>
			<toinfo></toinfo>
		</support>
	</format>
	<format id="json">
		<name>Log2Log JSON</name>
		<client>Log2Log</client>
		<class>JSON</class>
		<icon>:/images/services/JSON.png</icon>
		<support>
			<from>true</from>
			<to>true</to>
                        <specificity>-2</specificity>
			<accuracy>true</accuracy>
			<time>yes</time>
			<timezone>yes</timezone>
			<status>true</status>
			<system>true</system>
			<aliases>true</aliases>
			<group>true</group>
			<frominfo></frominfo>
			<toinfo>This dumps the converted chat logs into one Log2Log standard chat log.</toinfo>
		</support>
	</format>
	<format id="meeboconnect">
		<name>Meebo (download)</name>
		<client>Meebo</client>
		<class>MeeboConnect</class>
		<icon>:/images/services/MeeboConnect.png</icon>
		<support>
			<from>true</from>
			<to>false</to>
			<specificity>2</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>no</timezone>
			<status>false</status>
			<system>false</system>
			<aliases>true</aliases>
			<group>false</group>
			<frominfo>Enter your Meebo username and password.</frominfo>
			<toinfo></toinfo>
		</support>
	</format>
	<format id="meebo">
		<name>Meebo (files)</name>
		<client>Meebo</client>
		<class>Meebo</class>
		<icon>:/images/services/Meebo.png</icon>
		<support>
			<from>true</from>
			<to>true</to>
			<specificity>2</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>no</timezone>
			<status>false</status>
			<system>false</system>
			<aliases>true</aliases>
			<group>false</group>
			<frominfo>This requires raw Meebo chat log files.</frominfo>
			<toinfo>Filenames made in this format are structured to contain metadata, but the contents are exactly like raw Meebo chat logs.</toinfo>
		</support>
	</format>
	<format id="omegle">
		<name>Omegle HTML</name>
		<client>Omegle</client>
		<class>Omegle</class>
		<icon>:/images/services/Omegle.png</icon>
		<support>
			<from>true</from>
			<to>true</to>
                        <specificity>6</specificity>
			<accuracy>false</accuracy>
                        <time>group</time>
			<timezone>no</timezone>
			<status>false</status>
			<system>false</system>
			<aliases>false</aliases>
			<group>false</group>
			<frominfo>Talked to strangers?</frominfo>
			<toinfo>You want to convert to fake Omegle conversations?</toinfo>
		</support>
	</format>
	<format id="pidgin">
		<name>Pidgin HTML</name>
		<client>Pidgin</client>
		<class>Pidgin</class>
		<icon>:/images/services/Pidgin.png</icon>
		<support>
			<from>true</from>
			<to>true</to>
			<specificity>0</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>group</timezone>
			<status>true</status>
			<system>true</system>
			<aliases>true</aliases>
			<group>true</group>
			<frominfo>Pidgin poop?</frominfo>
			<toinfo></toinfo>
		</support>
	</format>
	<format id="skype">
		<name>Skype</name>
		<client>Skype</client>
		<class>Skype</class>
		<icon>:/images/services/Skype.png</icon>
		<support>
			<from>true</from>
			<to>false</to>
			<specificity>0</specificity>
			<accuracy>false</accuracy>
			<time>yes</time>
			<timezone>no</timezone>
			<status>false</status>
			<system>false</system>
			<aliases></aliases>
			<group>true</group>
			<frominfo></frominfo>
			<toinfo></toinfo>
		</support>
	</format>
</formats>
<!-- NOTE: <group> has yet to be designed in Log2Log Standard Chat Log Format. -->

